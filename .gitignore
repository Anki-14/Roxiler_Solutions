# Ignore node_modules for both frontend and backend
/backend/node_modules
/frontend/node_modules

# Ignore build folders (frontend build output)
frontend/build/

# Ignore build folders (backend build output, if any)
backend/build/

# Ignore environment variable files (.env)
backend/.env
frontend/.env

# Ignore log files
*.log

# Ignore temporary files (if any)
*.tmp
*.swp
*.swo

# Ignore IDE files
.vscode/
.idea/

# Ignore package-lock.json or yarn.lock (if you prefer to manage the dependencies manually)
package-lock.json
yarn.lock

# Ignore npm-debug log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore any other folders or files related to local development environments
/.env.local
/.env.development.local
/.env.production.local
